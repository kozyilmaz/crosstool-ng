CT_EXPERIMENTAL=y
CT_LOCAL_TARBALLS_DIR="${HOME}/src"
CT_SAVE_TARBALLS=y
CT_STRIP_TARGET_TOOLCHAIN_EXECUTABLES=y
CT_LOG_EXTRA=y
CT_ARCH_ARCH="mips32r2"
CT_ARCH_TUNE="24kec"
CT_ARCH_LE=y
CT_ARCH_FLOAT_SW=y
CT_TARGET_CFLAGS="-mdsp"
CT_ARCH_mips=y
CT_STATIC_TOOLCHAIN=y
CT_TARGET_VENDOR="mt762x"
CT_TARGET_ALIAS="mipsel-linux"
CT_KERNEL_linux=y
CT_CC_GCC_mips_plt=y
# CT_CC_GCC_ENABLE_TARGET_OPTSPACE is not set
CT_LIBC_musl=y
CT_LIBC_MUSL_OPTIMIZE_SPEED=y
CT_CC_LANG_CXX=y
CT_DEBUG_gdb=y
# CT_GDB_CROSS_PYTHON is not set
CT_GDB_CROSS_STATIC=y
